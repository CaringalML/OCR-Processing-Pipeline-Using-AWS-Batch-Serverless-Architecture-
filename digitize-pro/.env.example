# DigitizePro Environment Variables - Example Configuration
# Copy this file to .env and update with your actual values

# API Configuration
REACT_APP_API_GATEWAY_URL=https://your-api-gateway-url.amazonaws.com/prod
REACT_APP_API_VERSION=v1

# AWS Configuration (for reference, don't put actual credentials here)
REACT_APP_AWS_REGION=us-east-1
REACT_APP_S3_BUCKET_NAME=your-s3-bucket-name

# Application Configuration
REACT_APP_APP_NAME=DigitizePro
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_OCR=true

# Upload Configuration
REACT_APP_MAX_FILE_SIZE=524288000
REACT_APP_ALLOWED_FILE_TYPES=pdf,tiff,jpg,jpeg,png

# Pagination
REACT_APP_DEFAULT_PAGE_SIZE=20
REACT_APP_MAX_PAGE_SIZE=100